        <dependency>
            <id>log4j</id>
            <version>1.2.8</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>
        
        <dependency>
            <id>commons-beanutils</id>
            <version>1.7.0</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <id>commons-collections</id>
            <version>3.1</version>
            <properties>
                <war.bundle>true</war.bundle>
