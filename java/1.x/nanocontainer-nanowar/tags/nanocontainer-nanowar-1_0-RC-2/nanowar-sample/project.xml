        </dependency>

        <dependency>
            <id>proxytoys</id>
            <version>0.2-RC1</version>
            <properties>
                <war.bundle>false</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <id>xmlunit</id>
            <version>1.0</version>
            <properties>
                <war.bundle>false</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <id>jetty</id>
            <version>4.2.9</version>
            <properties>
                <war.bundle>false</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <groupId>tomcat</groupId>
            <artifactId>jasper-compiler</artifactId>
            <version>5.0.18</version>
            <properties>
                <war.bundle>false</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <groupId>tomcat</groupId>
            <artifactId>jasper-runtime</artifactId>
            <version>5.0.18</version>
            <properties>
                <war.bundle>false</war.bundle>
            </properties>
        </dependency>

