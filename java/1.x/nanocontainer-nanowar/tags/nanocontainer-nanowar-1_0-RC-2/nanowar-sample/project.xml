            <id>asm</id>
            <version>2.0</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <groupId>asm</groupId>
            <artifactId>asm-util</artifactId>
            <version>2.0</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>
        
        <dependency>
            <id>velocity</id>
            <version>1.4</version>
            <properties>
                <war.bundle>true</war.bundle>
