<?xml version="1.0" encoding="UTF-8"?>

<project>
    <extend>${basedir}/../project.xml</extend>
    <id>nanocontainer-nanowar-sample</id>
    <name>NanoContainer NanoWar Sample</name>

    <dependencies>
        <dependency>
            <id>picocontainer</id>
            <version>${picocontainer.version}</version>
            <url>http://www.picocontainer.org/</url>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <groupId>picocontainer</groupId>
            <artifactId>picocontainer-tck</artifactId>
            <version>${picocontainer.version}</version>
            <url>http://www.picocontainer.org/</url>
        </dependency>

        <dependency>
            <id>servletapi</id>
            <version>2.3</version>
        </dependency>

        <dependency>
            <groupId>nanocontainer</groupId>
            <artifactId>nanocontainer</artifactId>
            <version>${nanocontainer.version}</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <groupId>nanocontainer</groupId>
            <artifactId>nanocontainer-nanowar</artifactId>
            <version>${pom.currentVersion}</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>
        
        <dependency>
            <id>xstream</id>
            <version>1.0.2</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <!--
        Groovy and ASM only needed if using Groovy to
        assemble the container in web.xml
        -->
        <dependency>
            <id>groovy</id>
            <version>1.0-jsr-03</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <id>antlr</id>
            <version>2.7.5</version>
            <properties>
                <war.bundle>true</war.bundle>
