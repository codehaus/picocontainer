<project>
    <extend>${basedir}/../project.xml</extend>
    <id>nanocontainer-ejb</id>
    <name>nanocontainer-ejb</name>

    <dependencies>
        <dependency>
            <id>picocontainer</id>
            <version>${picocontainer.version}</version>
            <url>http://www.picocontainer.org/</url>
        </dependency>

        <dependency>
            <groupId>picocontainer</groupId>
            <artifactId>picocontainer-tck</artifactId>
            <version>${picocontainer.version}</version>
            <url>http://www.picocontainer.org/</url>
        </dependency>

        <dependency>
            <groupId>nanocontainer</groupId>
            <artifactId>nanocontainer-proxytoys</artifactId>
            <version>${pom.currentVersion}</version>
        </dependency>

        <dependency>
            <id>proxytoys</id>
            <version>0.1</version>
        </dependency>

        <!-- J2EE dependency, replace with you server's client jar -->
        <dependency>
            <groupId>jboss</groupId>
            <artifactId>jboss-j2ee</artifactId>
            <version>3.2.3</version>
        </dependency>
        <!-- test dependencies -->
        <dependency>
            <groupId>jmock</groupId>
            <artifactId>jmock</artifactId>
            <version>1.0.1</version>
        </dependency>
        <dependency>
            <groupId>jmock</groupId>
            <artifactId>jmock-cglib</artifactId>
            <version>1.0.1</version>
        </dependency>
        <dependency>
            <groupId>cglib</groupId>
            <artifactId>cglib-full</artifactId>
            <version>2.0.1</version>
        </dependency>
    </dependencies>
</project>

