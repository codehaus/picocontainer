<?xml version="1.0" encoding="UTF-8"?>

<project>
    <extend>${basedir}/../../project.xml</extend>
    <id>nanocontainer-nanowar-sample-webwork2</id>
    <name>NanoContainer NanoWar Sample WebWork2</name>

    <dependencies>
       
        <dependency>
            <groupId>opensymphony</groupId>
            <artifactId>webwork2</artifactId>
            <jar>webwork-2.1.7.jar</jar>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <groupId>opensymphony</groupId>
            <artifactId>xwork</artifactId>
            <version>1.0.5</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <groupId>opensymphony</groupId>
            <artifactId>webwork1</artifactId>
            <jar>webwork-1.4.jar</jar>
            <properties>
                <war.bundle>false</war.bundle>
            </properties>
        </dependency>
        
        <dependency>
            <groupId>opensymphony</groupId>
            <artifactId>oscore</artifactId>
            <version>2.2.4</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

    </dependencies>
    
	<build>
        <sourceDirectory>${basedir}/../../src/java</sourceDirectory>
		<sourceModifications>
			<sourceModification>
				<className>org.nanocontainer.NonExistent</className>
		      	<includes>
		        		<include>org/nanocontainer/nanowar/sample/**</include>
		      	</includes>
		      	<excludes>
		        		<exclude>org/nanocontainer/nanowar/sample/struts/**</exclude>
		      	</excludes>
			</sourceModification>
		</sourceModifications>
    </build>
    
</project>
