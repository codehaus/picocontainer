    <repository>
        <connection>scm:cvs:pserver:anonymous@cvs.codehaus.org:/cvsroot/nanocontainer:nano</connection>
        <url>http://nanocontainer.cvs.codehaus.org/nano/</url>
    </repository>

    <versions>
        <version>
            <id>1.0-alpha-1</id>
            <name>1.0-alpha-1</name>
            <tag>NANOCONTAINER_1_0_ALPHA_1</tag>
        </version>
        <version>
            <id>1.0-alpha-2</id>
            <name>1.0-alpha-2</name>
            <tag>HEAD</tag>
        </version>
    </versions>

    <mailingLists>
        <mailingList>
            <name>NanoContainer Developers</name>
