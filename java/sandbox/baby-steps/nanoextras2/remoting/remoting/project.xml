<?xml version="1.0" encoding="UTF-8"?>

<project>
    <extend>${basedir}/../project.xml</extend>
    <id>nanocontainer-remoting</id>
    <name>Nanocontainer Remoting</name>

    <dependencies>
        <dependency>
            <id>picocontainer</id>
            <version>${picocontainer.version}</version>
            <url>http://www.picocontainer.org/</url>
        </dependency>

        <dependency>
            <groupId>picocontainer</groupId>
            <artifactId>picocontainer-gems</artifactId>
            <version>${picocontainer.version}</version>
            <url>http://www.picocontainer.org/</url>
        </dependency>

        <dependency>
            <groupId>picocontainer</groupId>
            <artifactId>picocontainer-tck</artifactId>
            <version>${picocontainer.version}</version>
            <url>http://www.picocontainer.org/</url>
        </dependency>

        <!-- J2EE dependency, replace with you server's client jar -->
        <dependency>
            <groupId>geronimo-spec</groupId>
            <artifactId>geronimo-spec-ejb</artifactId>
            <version>2.1-rc4</version>
        </dependency>

        <dependency>
            <groupId>mx4j</groupId>
            <artifactId>mx4j-jmx</artifactId>
            <version>2.1.1</version>
        </dependency>

        <dependency>
            <groupId>mx4j</groupId>
            <artifactId>mx4j-impl</artifactId>
            <version>2.1.1</version>
        </dependency>

	<dependency>
            <id>jmock</id>
            <version>1.0.1</version>
        </dependency>

        <dependency>
            <groupId>jmock</groupId>
            <artifactId>jmock-cglib</artifactId>
            <version>1.0.1</version>
        </dependency>

        <dependency>
            <groupId>cglib</groupId>
            <artifactId>cglib-nodep</artifactId>
            <version>2.1_3</version>
        </dependency>

        <dependency>
            <id>proxytoys</id>
            <version>0.2.1</version>
        </dependency>

    </dependencies>

</project>
