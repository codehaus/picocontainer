<project>
    <extend>${basedir}/../project.xml</extend>
    <id>nanocontainer-ejb</id>
    <name>nanocontainer-ejb</name>

    <dependencies>
        <dependency>
            <id>picocontainer</id>
            <version>${picocontainer.version}</version>
            <url>http://www.picocontainer.org/</url>
        </dependency>

        <dependency>
            <groupId>picocontainer</groupId>
            <artifactId>picocontainer-gems</artifactId>
            <version>${picocontainer.version}</version>
            <url>http://www.picocontainer.org/</url>
        </dependency>

        <dependency>
            <id>proxytoys</id>
            <version>0.1</version>
        </dependency>

        <!-- J2EE dependency, replace with you server's client jar -->
