<project
  default="build-subprojects"
  xmlns:maven="jelly:maven">

  <goal 
    name="build-subprojects"
    description="Build each sub project">

    <maven:reactor
      basedir="${basedir}"
      includes="*/project.xml"
      excludes="picogui/*,picoidea/*,temp/*"
      goals="jar:install"
      banner="Building"
      ignoreFailures="false"
    />

  </goal>

</project>
