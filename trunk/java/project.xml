<?xml version="1.0" encoding="UTF-8"?>

<project>
  <pomVersion>3</pomVersion>
  <id>nanocontainer</id>
  <name>NanoContainer</name>
  <currentVersion>1.0-SNAPSHOT</currentVersion>

  <organization>
    <name>Codehaus</name>
    <url>http://codehaus.org/</url>
    <logo>http://images.werken.com/codehaus-plain.gif</logo>
  </organization>
  <inceptionYear>2003</inceptionYear>
  <package>nanocontainer</package>
  <logo>/images/nanocontainer.jpg</logo>
  <description>Extension of PicoContainer</description>
  <shortDescription>Extension of PicoContainer</shortDescription>
  <url>http://www.nanocontainer.org/</url>
  <issueTrackingUrl>http://jira.codehaus.org/secure/BrowseProject.jspa?id=10121</issueTrackingUrl>
  <siteAddress>codehaus.org</siteAddress>
  <siteDirectory>/www/nanocontainer.codehaus.org</siteDirectory>
  <distributionDirectory>/www/nanocontainer.codehaus.org/builds/</distributionDirectory>
  <repository>
    <connection>scm:cvs:pserver:anonymous@cvs.codehaus.org:/cvsroot/nanocontainer:nano</connection>
    <url>http://cvs.codehaus.org/viewcvs.cgi/nano/</url>
  </repository>

  <mailingLists>
    <mailingList>
      <name>Nanocontainer Developers</name>
      <subscribe>http://lists.codehaus.org/mailman/listinfo/nanocontainer-dev</subscribe>
      <unsubscribe>http://lists.codehaus.org/mailman/listinfo/nanocontainer-dev</unsubscribe>
      <archive>http://lists.codehaus.org/pipermail/nanocontainer-dev/</archive>
    </mailingList>
    <mailingList>
      <name>Nanocontainer CVS Commits</name>
      <subscribe>http://lists.codehaus.org/mailman/listinfo/nanocontainer-commits</subscribe>
      <unsubscribe>http://lists.codehaus.org/mailman/listinfo/nanocontainer-commits</unsubscribe>
      <archive>http://lists.codehaus.org/pipermail/nanocontainer-commits/</archive>
    </mailingList>
  </mailingLists>

  <developers>
    <developer>
      <name>Paul Hammant</name>
      <id>paul</id>
      <email>paul_hammant@yahoo.com</email>
      <organization></organization>
      <roles>
        <role>Java Developer</role>
      </roles>
    </developer>
    <developer>
      <name>Aslak Hellesoy</name>
      <id>rinkrank</id>
      <email>rinkrank@codehaus.org</email>
      <organization></organization>
      <roles>
        <role>Java Developer</role>
      </roles>
    </developer>
    <developer> 
      <name>Jon Tirsen</name> 
      <id>tirsen</id> 
      <email>tirsen@codehaus.org</email> 
      <organization></organization> 
      <roles> 
        <role>Java Developer</role> 
      </roles> 
    </developer> 
    <developer> 
      <name>Joe Walnes</name> 
      <id>joe</id> 
      <email>joe@codehaus.org</email> 
      <organization></organization> 
      <roles> 
        <role>Java Developer</role> 
      </roles> 
    </developer> 
  </developers>

  <dependencies>
    <dependency>
      <id>picocontainer</id>
      <version>1.0-SNAPSHOT</version>
      <url>http://picocontainer.codehaus.org/</url>
    </dependency>

    <dependency>
      <id>commons-collections</id>
      <version>2.1</version>
      <url>http://jakarta.apache.org/</url>
    </dependency>

    <dependency>
      <id>nanning</id>
      <version>0.8</version>
      <url>http://nanning.sourceforge.net/</url>
    </dependency>

    <dependency>
      <id>servletapi</id>
      <version>2.3</version>
      <url>http://java.sun.com/products/servlet/</url>
    </dependency>

    <dependency>
      <id>webwork</id>
      <version>1.3</version>
      <url>http://www.opensymphony.com/webwork/</url>
    </dependency>

    <dependency>
      <id>commons-digester</id>
      <version>1.4.1</version>
      <url>http://jakarta.apache.org/commons/digester/</url>
    </dependency>

    <dependency>
      <id>commons-logging</id>
      <version>1.0.1</version>
      <url>http://jakarta.apache.org/commons/logging/</url>
    </dependency>

    <dependency>
      <id>commons-beanutils</id>
      <version>1.6.1</version>
      <url>http://jakarta.apache.org/</url>
    </dependency>

  </dependencies>

  <build>
    <nagEmailAddress>nano-dev@codehaus.org</nagEmailAddress>
    <sourceDirectory>${basedir}/src/java</sourceDirectory>
    <unitTestSourceDirectory>${basedir}/src/test</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*TestCase.java</include>
      </includes>
    </unitTest>
    <resources>
      <resource>
        <directory>${basedir}/src/conf</directory>
        <includes>
          <include>*.properties</include>
        </includes>
      </resource>
    </resources>
  </build>

  <reports> 
    <report>maven-license-plugin</report> 
    <report>maven-checkstyle-plugin</report> 
    <report>maven-pmd-plugin</report> 
    <report>maven-simian-plugin</report> 
    <report>maven-jdepend-plugin</report> 
    <report>maven-changelog-plugin</report> 
    <report>maven-statcvs-plugin</report>
    <report>maven-file-activity-plugin</report> 
    <report>maven-developer-activity-plugin</report> 
    <report>maven-jxr-plugin</report> 
    <report>maven-javadoc-plugin</report> 
    <report>maven-junit-report-plugin</report> 
    <report>maven-clover-plugin</report> 
  </reports> 

</project>
