<?xml version="1.0" encoding="UTF-8"?>

<project>
    <extend>${basedir}/../project.xml</extend>
    <id>nanocontainer-sample-struts</id>
    <name>nanocontainer-sample-struts</name>

    <dependencies>

        <dependency>
            <id>servletapi</id>
            <version>2.3</version>
        </dependency>

        <dependency>
            <groupId>nanocontainer</groupId>
            <artifactId>nanocontainer</artifactId>
            <version>${pom.currentVersion}</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <groupId>nanocontainer</groupId>
            <artifactId>nanocontainer-servlet</artifactId>
            <version>${pom.currentVersion}</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <groupId>nanocontainer</groupId>
            <artifactId>nanocontainer-struts</artifactId>
            <version>${pom.currentVersion}</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <id>groovy</id>
            <version>1.0-beta-5</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <id>asm</id>
            <version>1.4.1</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <id>asm</id>
            <artefactId>asm-util</artefactId>
            <version>1.4.1</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <id>struts</id>
            <version>1.1</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <id>commons-collections</id>
            <version>2.1</version>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <id>commons-beanutils</id>
            <version>1.6.1</version>
            <url>http://jakarta.apache.org/</url>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

        <dependency>
            <id>commons-digester</id>
            <version>1.4.1</version>
            <url>http://jakarta.apache.org/commons/digester/</url>
            <properties>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>

    </dependencies>

</project>
